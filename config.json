{
  "name" :"mysql-test-connector",
  "connector.class": "io.debezium.connector.mysql.MySqlConnector",
  "snapshot.locking.mode": "none",
  "transforms.unwrap.delete.handling.mode": "rewrite",
  "transforms.insertKey.fields": "your_key_field",
  "tasks.max": "1",
  "database.history.kafka.topic": "your_topic",
  "transforms": "unwrap,insertKey,extractKey",
  "transforms.extractKey.type": "org.apache.kafka.connect.transforms.ExtractField$Key",
  "include.schema.changes": "false",
  "transforms.unwrap.drop.tombstones": "true",
  "database.history.skip.unparseable.ddl": "true",
  "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
  "database.user": "debezium",
  "transforms.extractKey.field": "your_key_field",
  "database.server.id": "your_server_id",
  "transforms.insertKey.type": "org.apache.kafka.connect.transforms.ValueToKey",
  "database.history.kafka.bootstrap.servers": "your_kafka",
  "database.server.name": "your_db_server_name",
  "internal.database.history.ddl.filter": "DROP TEMPORARY TABLE IF EXISTS .+ /* generated by server /,INSERT INTO mysql.rds_heartbeat2(.) values (.*) ON DUPLICATE KEY UPDATE value = .*,DELETE FROM mysql.rds_sysinfo.*,INSERT INTO mysql.rds_sysinfo(.*) values (.*),DELETE FROM mysql.rds_monitor.*,FLUSH RELAY LOGS.*,flush relay logs.*,SAVEPOINT .*,# Dummy event replacing.*",
  "transforms.unwrap.add.headers": "op",
  "database.port": "your_db_port",
  "database.hostname": "your_db_host",
  "database.password": "debezium",
  "database.history.store.only.monitored.tables.ddl": "true",
  "table.include.list": "your_database.your_table",
  "database.include.list": "your_database",
  "snapshot.mode": "schema_only"
}
